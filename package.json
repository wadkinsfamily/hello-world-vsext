{
  "name": "hello-world",
  "displayName": "Hello World",
  "publisher": "wadkins-family",
  "version": "0.0.2",
  "description": "Minimal Hello World extension used for VSIX and Marketplace testing.",
  "keywords": ["hello","vscode","extension"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wadkinsfamily/hello-world-vsext.git"
  },
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": ["Other"],
  "activationEvents": ["onCommand:hello-world.helloWorld"],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "hello-world.helloWorld",
        "title": "Hello World: Hello"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "publish:vsce": "npx vsce publish",
    "package": "vsce package --no-dependencies",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "@types/node": "^22.0.0",
    "@types/vscode": "^1.105.0",
    "vsce": "^2.10.0"
  },
  "dependencies": {}
}
